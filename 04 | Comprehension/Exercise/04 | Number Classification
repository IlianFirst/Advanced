numbers = [int(number) for number in input().split(", ")]
print(f"Positive: {', '.join(map(str, [number for number in numbers if number >= 0]))}")
print(f"Negative: {', '.join(map(str, [number for number in numbers if number < 0]))}")
print(f"Even: {', '.join(map(str, [number for number in numbers if number % 2 == 0]))}")
print(f"Odd: {', '.join(map(str, [number for number in numbers if number % 2 == 1]))}")
