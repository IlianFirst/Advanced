command = input()
numbers = [int(i) for i in input().split()]
if command == "Odd":
    sum_of_odd_numbers = sum(filter(lambda x: x % 2 == 1, numbers)) * len(numbers)
    print(sum_of_odd_numbers)
else:
    sum_of_even_numbers = sum(filter(lambda x: x % 2 == 0, numbers)) * len(numbers)
    print(sum_of_even_numbers)
